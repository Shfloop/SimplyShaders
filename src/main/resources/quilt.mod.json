{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "Adds more complex shader support",
      "contributors": {
        "Your name here": "Shfloop"
      },

      "contact": {

        "issues": "https://github.com/Shfloop/SimplyShaders/issues",
        "sources": "https://github.com/Shfloop/SimplyShaders"
      },

      "icon": "assets/example_mod/icon.png"
    },

    "entrypoints": {
      "init": "com.shfloop.simply_shaders.SimplyShaders"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "simply_shaders.mixins.json"
}
