{
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "Adds more complex shader support",
  "authors": [
    "Shfloop"
  ],
  "meta": {
    "icon": "${mod_id}:assets/example-mod/icons/icon.png"
  },
  "entrypoints": {
    "client_init": [
      "com.shfloop.simply_shaders.SimplyShadersPuzzle"
    ]
  },
  "mixins": [
    "simply_shaders.mixins.json",
    {
      "config":"simply_shaders.puzzle.mixins.json",
      "environment": "client"
    }

  ],
  "dependencies": {
    "cosmic-reach": ">=0.3.6",
    "puzzle-loader": ">=2.0.9"
  },
  "accessManipulator": "simply_shaders.manipulator"
}
